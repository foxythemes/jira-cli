    "inquirer": "^6.5.2",
    "jira-client": "^4.2.0",
    "moment": "^2.27.0",
    "opn": "^5.5.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
